{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"downlevelIteration": true,
		"module": "esnext",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"skipLibCheck": false,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		]
	}
}
